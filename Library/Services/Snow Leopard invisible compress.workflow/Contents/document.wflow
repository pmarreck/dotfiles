				<key>ActionName</key>
				<string>Run Shell Script</string>
				<key>ActionParameters</key>
				<dict>
					<key>COMMAND_STRING</key>
					<string>for f in "$@"
do
	fout=${f%/}  # strip trailing slash
	path=${fout%/*} # strip everything after the last slash which includes the filename
	cd "$path/"
	filename=${fout##/*/}  # deletes entire path before last filename, hopefully
	authenticatedASaction="do shell script \"ditto --hfsCompression --zlibCompressionLevel=9 '$filename' '$filename.tmp'; rm -rf '$filename'; mv '$filename.tmp' '$filename'\" with administrator privileges"
	osascript -e "$authenticatedASaction"
	echo "$f &gt; $filename (internallycompressed)" &gt;&gt; $HOME/snowleopardcompressedfiles.log
done
</string>
